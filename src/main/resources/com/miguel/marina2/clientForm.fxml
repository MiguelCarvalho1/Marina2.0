<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.miguel.marina2.ClientFormController" prefHeight="400.0" prefWidth="600.0">
    <children>
        <GridPane layoutX="-7.0" layoutY="-4.0" prefHeight="400.0" prefWidth="600.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Label text="Id">
                    <GridPane.margin>
                        <Insets left="25.0" top="5.0" />
                    </GridPane.margin>
                </Label>
                <Label text="Name" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets left="25.0" top="5.0" />
                    </GridPane.margin>
                </Label>
                <Label text="Email" GridPane.rowIndex="2">
                    <GridPane.margin>
                        <Insets left="25.0" top="5.0" />
                    </GridPane.margin>
                </Label>
                <Label text="Phone" GridPane.rowIndex="3">
                    <GridPane.margin>
                        <Insets left="25.0" top="5.0" />
                    </GridPane.margin>
                </Label>
                <TextField fx:id="txtId" GridPane.columnIndex="1" />
                <TextField fx:id="txtName" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <TextField fx:id="txtEmail" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                <TextField fx:id="txtPhone" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                <Label fx:id="labelErrorName" textFill="#f10101" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                <Label fx:id="labelErrorCode" textFill="#f10101" GridPane.columnIndex="2" GridPane.rowIndex="2" />
                <Label fx:id="labelErrorPhone" textFill="#f10101" GridPane.columnIndex="2" GridPane.rowIndex="3" />
                <HBox prefHeight="100.0" prefWidth="200.0" spacing="5.0" GridPane.columnIndex="1" GridPane.rowIndex="4">
                    <padding>
                        <Insets left="5.0" top="5.0" />
                    </padding>
                    <children>
                        <Button fx:id="btSave" mnemonicParsing="false" onAction="#onBtSaveAction" text="Save" />
                        <Button fx:id="btCancel" mnemonicParsing="false" onAction="#onBtCancelAction" text="Cancel" />
                    </children>
                </HBox>
            </children>
        </GridPane>
    </children>
</AnchorPane>
